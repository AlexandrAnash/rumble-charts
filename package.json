{
  "name": "rumble-charts",
  "version": "0.1.4",
  "description": "React charts components based on d3.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib",
    "test": "./node_modules/.bin/jest",
    "publish-patch": "npm run build && npm version patch && npm publish",
    "publish-minor": "npm run build && npm version minor && npm publish",
    "publish-major": "npm run build && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RumbleInc/rumble-js-charts.git"
  },
  "keywords": [
    "react",
    "d3",
    "charts"
  ],
  "author": "Alexey Volkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RumbleInc/rumble-js-charts/issues"
  },
  "homepage": "https://github.com/RumbleInc/rumble-js-charts#readme",
  "dependencies": {
    "d3": "3.5.6",
    "d3-cloud": "1.2.1",
    "lodash": "3.10.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-jest": "10.0.1",
    "babel-plugin-transform-react-jsx": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "enzyme": "2.2.0",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "^4.2.3",
    "jest": "0.1.40",
    "jest-cli": "0.10.0",
    "react": "0.14.x",
    "react-addons-test-utils": "0.14.x",
    "react-dom": "0.14.x"
  },
  "peerDependencies": {
    "react": "0.14.x",
    "react-dom": "0.14.x"
  },
  "jest": {
    "automock": false,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testPathDirs": [
      "<rootDir>/tests/components"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.values",
      "es-abstract"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fakeData/"
    ],
    "setupTestFrameworkScriptFile": "./tests/init.js"
  }
}
